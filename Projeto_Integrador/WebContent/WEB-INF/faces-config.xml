<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">




	<navigation-rule>
		<from-view-id>indexEMPREGADOR.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>showEMPREGADO</from-outcome>
			<to-view-id>showEMPREGADO.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>indexEMPREGADOR.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>novaMensagemEMPREGADO</from-outcome>
			<to-view-id>novaMensagemEMPREGADO.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>showEMPREGADO.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>indexEMPREGADOR</from-outcome>
			<to-view-id>indexEMPREGADOR.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>novaMensagemEMPREGADO.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>indexEMPREGADOR</from-outcome>
			<to-view-id>indexEMPREGADOR.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>



	<navigation-rule>
		<from-view-id>showMsgEMPREGADO</from-view-id>
		<navigation-case>
			<from-outcome>listamensagemEMPREGADO</from-outcome>
			<to-view-id>listamensagemEMPREGADO.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	
		<navigation-rule>
		<from-view-id>showEMPREGADO.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>novaMensagemEMPREGADO</from-outcome>
			<to-view-id>novaMensagemEMPREGADO.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>



</faces-config>
